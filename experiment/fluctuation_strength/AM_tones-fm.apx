                </trial>
            </trials>
        </procedure>
    </procedure>
    <corrector xsi:type="apex:alternatives">
        <answers>
            <answer number="1" value="button11"/>
            <answer number="2" value="button12"/>
            <answer number="3" value="button13"/>
            <answer number="4" value="button14"/>
            <answer number="5" value="button15"/>
            <answer number="6" value="button16"/>
            <answer number="7" value="button17"/>
            <answer number="8" value="button18"/>
            <answer number="9" value="button19"/>
        </answers>
    </corrector>
    <screens>
        <general>
            <repeatbutton>true</repeatbutton>
            <showpanel>true</showpanel>
            <showmenu>true</showmenu>
        </general>
        <reinforcement>
            <progressbar>true</progressbar>
            <feedback>false</feedback>
        </reinforcement>
        <screen id="screen1">
            <gridLayout height="3" width="1" id="main_layout">
                <gridLayout width="3" height="1" columnstretch="1,2,1" rowstretch="1" y="1">
                    <label id="label_1" x="2" y="1">
                        <text>How much does the second stimulus fluctuate with respect to the first one?</text>
                    </label>
                </gridLayout>
                <gridLayout width="9" height="1" columnstretch="1,1,1,1,1,1,1,1,1" rowstretch="1" y="2">
                    <button id="button11" x="1" y="1">
                        <text>0</text>
                    </button>
                    <button id="button12" x="2" y="1">
                        <text>25</text>
                    </button>
                    <button id="button13" x="3" y="1">
                        <text>50</text>
                    </button>
                    <button id="button14" x="4" y="1">
                        <text>75</text>
                    </button>
                    <button id="button15" x="5" y="1">
                        <text>100</text>
                    </button>
                    <button id="button16" x="6" y="1">
                        <text>125</text>
                    </button>
                    <button id="button17" x="7" y="1">
                        <text>150</text>
                    </button>
                    <button id="button18" x="8" y="1">
                        <text>175</text>
                    </button>
                    <button id="button19" x="9" y="1">
                        <text>200</text>
                    </button>
                </gridLayout>
                <gridLayout width="3" height="1" rowstretch="1" columnstretch="1,2,1" y="3"></gridLayout>
            </gridLayout>
            <buttongroup id="btgrp1">
                <button id="button11"/>
                <button id="button12"/>
                <button id="button13"/>
                <button id="button14"/>
                <button id="button15"/>
                <button id="button16"/>
                <button id="button17"/>
                <button id="button18"/>
                <button id="button19"/>
            </buttongroup>
            <default_answer_element>btgrp1</default_answer_element>
        </screen>
    </screens>
    <datablocks>
        <uri_prefix source="apexconfig">stimuli</uri_prefix>
        <datablock id="datablockcaltone">
            <device>wavdevice</device>
            <uri>tone-fc_1000-SPL_0-f_-44100-N_441000.wav</uri>
        </datablock>
        <datablock id="datablockref1">
            <device>wavdevice</device>
            <uri>AM_tone-fm_4.00-fc_1000-md_40-SPL_70-w_25-fs_44100-N_88200.wav</uri>
        </datablock>
        <datablock id="datablockref2">
            <device>wavdevice</device>
            <uri>AM_tone-fm_0.50-fc_1000-md_40-SPL_70-w_25-fs_44100-N_264600.wav</uri>
        </datablock>
        <datablock id="datablock1">
            <device>wavdevice</device>
            <uri>AM_tone-fm_0.50-fc_1000-md_40-SPL_70-w_25-fs_44100-N_264600.wav</uri>
        </datablock>
        <datablock id="datablock2">
            <device>wavdevice</device>
            <uri>AM_tone-fm_1.00-fc_1000-md_40-SPL_70-w_25-fs_44100-N_132300.wav</uri>
