                    <standard id="stimref1"/>
                </trial>
                <trial id="trial17">
                    <screen id="screen1"/>
                    <stimulus id="stim7"/>
                    <standard id="stimref1"/>
                </trial>
            </trials>
        </procedure>
        <procedure xsi:type="apex:constantProcedureType" id="procedure2">
            <parameters>
                <presentations>4</presentations>
                <order>random</order>
                <choices select="2">2</choices>
                <pause_between_stimuli>800</pause_between_stimuli>
            </parameters>
            <trials>
                <trial id="trial21">
                    <screen id="screen1"/>
                    <stimulus id="stim1"/>
                    <standard id="stimref2"/>
                </trial>
                <trial id="trial22">
                    <screen id="screen1"/>
                    <stimulus id="stim2"/>
                    <standard id="stimref2"/>
                </trial>
                <trial id="trial23">
                    <screen id="screen1"/>
                    <stimulus id="stim3"/>
                    <standard id="stimref2"/>
                </trial>
                <trial id="trial24">
                    <screen id="screen1"/>
                    <stimulus id="stim4"/>
                    <standard id="stimref2"/>
                </trial>
                <trial id="trial25">
                    <screen id="screen1"/>
                    <stimulus id="stim5"/>
                    <standard id="stimref2"/>
                </trial>
                <trial id="trial26">
                    <screen id="screen1"/>
                    <stimulus id="stim6"/>
                    <standard id="stimref2"/>
                </trial>
                <trial id="trial27">
