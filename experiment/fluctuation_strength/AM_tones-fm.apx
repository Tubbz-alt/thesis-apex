    <connections>
        <connection>
            <from>
                <id>_ALL_</id>
                <channel>0</channel>
            </from>
            <to>
                <id>ampMain</id>
                <channel>0</channel>
            </to>
        </connection>
        <connection>
            <from>
                <id>ampMain</id>
                <channel>0</channel>
            </from>
            <to>
                <id>wavdevice</id>
                <channel>0</channel>
            </to>
        </connection>
    </connections>
    <calibration profile="calprofile1">
        <stimuli>
            <stimulus id="stimcaltone"/>
        </stimuli>
        <parameters>
            <parameter id="gain0">
                <targetamplitude>60</targetamplitude>
                <calibrationamplitude>65</calibrationamplitude>
                <mute>-150.0</mute>
                <max>50</max>
            </parameter>
        </parameters>
    </calibration>
    <results>
        <xsltscript>apexresult.xsl</xsltscript>
        <saveprocessedresults>true</saveprocessedresults>
    </results>
    <general>
        <autosave>true</autosave>
    </general>
</apex:apex>
